{
    "version": 2,
    "trailingSlash": false,
    "redirects": [
      {
        "source": "/(.*)",
        "has": [
          {
            "type": "host",
            "value": "daydreamforge.com"
          }
        ],
        "destination": "https://www.daydreamforge.com/$1",
        "permanent": true
      }
    ]
  }
  